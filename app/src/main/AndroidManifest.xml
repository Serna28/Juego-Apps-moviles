<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.upb.juego_disparos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Juego_Disparos">
        <activity
            android:name="com.upb.juego_disparos.Puntajes"
            android:exported="false" />
        <activity
            android:name="com.upb.juego_disparos.gameover"
            android:exported="false" />
        <activity
            android:name="com.upb.juego_disparos.Escenario"
            android:exported="false" />
        <activity
            android:name="com.upb.juego_disparos.Menu"
            android:exported="false" />
        <activity
            android:name="com.upb.juego_disparos.Login"
            android:exported="false" />
        <activity
            android:name="com.upb.juego_disparos.Registro"
            android:exported="false" />
        <activity
            android:name="com.upb.juego_disparos.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.upb.juego_disparos.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>